{
	"name": "express-authorshaven",
	"version": "1.0.0",
	"description": "A Social platform for the creative at heart",
	"main": "index.js",
	"scripts": {
		"build": "babel -d ./build ./src -s",
		"prod": "set NODE_ENV=production && node build/index.js",
		"start": "set NODE_ENV=development && nodemon --exec babel-node src/index.js",
		"dev": "set DEBUG=web-app && set NODE_ENV=development && nodemon src/index.js --exec babel-node",
		"test": "set NODE_ENV=test& mocha --timeout 1000 ./src/tests/*.js --require @babel/register --reporter spec --exec babel-node --exit",
		"coverage": "nyc report --reporter=text-lcov | coveralls",
		"coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
	},
	"author": "Andela Simulations Programme",
	"license": "MIT",
	"dependencies": {
		"@babel/runtime": "^7.5.5",
		"cors": "^2.8.4",
		"coveralls": "^3.0.3",
		"debug": "^4.1.1",
		"dotenv": "^6.2.0",
		"ejs": "^2.6.1",
		"errorhandler": "^1.5.0",
		"express": "^4.16.3",
		"express-jwt": "^5.3.1",
		"express-session": "^1.15.6",
		"jsonwebtoken": "^8.3.0",
		"method-override": "^2.3.10",
		"methods": "^1.1.2",
		"morgan": "^1.9.1",
		"passport": "^0.4.0",
		"passport-local": "^1.0.0",
		"path": "^0.12.7",
		"request": "^2.87.0",
		"sequelize": "^5.15.2",
		"underscore": "^1.9.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.5.5",
		"@babel/core": "^7.5.5",
		"@babel/node": "^7.5.5",
		"@babel/plugin-transform-runtime": "^7.5.5",
		"@babel/preset-env": "^7.5.5",
		"@babel/register": "^7.5.5",
		"chai": "^4.2.0",
		"chai-http": "^4.3.0",
		"eslint": "^6.1.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.14.3",
		"eslint-plugin-react-hooks": "^1.7.0",
		"mocha": "^6.2.0",
		"nodemon": "^1.19.1"
	}
}
